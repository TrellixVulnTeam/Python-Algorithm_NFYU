   "source": [
    "# 이분탐색으로도 풀 수 있는 문제\n",
    "# 시간복잡도에 차이가 존재\n",
    "\n",
    "n = int(input())\n",
    "n_list = list(map(int, input().split()))\n",
    "dp = [1 for i in range(n)]\n",
    "\n",
    "for i in range(n):\n",
    "    for j in range(i):\n",
    "        if n_list[i] > n_list[j]:\n",
    "            dp[i] = max(dp[i], dp[j] + 1)\n",
    "\n",
    "print(max(dp))\n"
   ]
  }
