    "# n = (n - 1) + (n - 5)로 구할 수 있음. 방법 알알보기\n",
    "n = int(input())\n",
    "dp = [0] * 101\n",
    "dp[1] = 1\n",
    "dp[2] = 1\n",
    "dp[3] = 1\n",
    "for i in range(n):\n",
    "    num = int(input())\n",
    "    for j in range(4, num + 1):\n",
    "        dp[j] = dp[j - 2] + dp[j - 3]\n",
    "    print(dp[num])\n",
    "    "
   ]
