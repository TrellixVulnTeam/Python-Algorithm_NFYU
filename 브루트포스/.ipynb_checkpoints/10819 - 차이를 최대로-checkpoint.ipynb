   "source": [
    "from itertools import permutations\n",
    "import sys\n",
    "\n",
    "n = int(input())\n",
    "a = list(map(int, input().split()))\n",
    "\n",
    "per = permutations(a)\n",
    "answer = 0\n",
    "\n",
    "for i in per:\n",
    "    s = 0\n",
    "    for j in range(len(i) - 1):\n",
    "        s += abs(i[j] - i[j + 1])\n",
    "    if s > answer:\n",
    "        answer = s\n",
    "\n",
    "print(answer)"
   ]
